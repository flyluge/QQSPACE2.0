<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.qqSpace.domain.Praise" table="praise" schema="qqspace">
        <id name="pid">
            <column name="pid" sql-type="int(11)"/>
        </id>
        <property name="uid">
            <column name="uid" sql-type="int(11)" />
        </property>
        <property name="aid">
            <column name="aid" sql-type="int(11)" />
        </property>
    </class>
</hibernate-mapping>